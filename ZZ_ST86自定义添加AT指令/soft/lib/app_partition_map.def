# Ext Ram/Falsh memory map for App Partition 
ifeq "${APART}" "1"
ifeq "${COMPRESS}" "1"
SL_FUNCTAB_BASE     		:= 0xFFFFFFFF82230000
SL_FUNCTAB_SIZE     		:= 0x800
SL_APP_BASE         		:= 0xFFFFFFFF82230800
SL_APP_LOADINFO_BASE		:= 0xFFFFFFFF88320000
SL_APP_LOADINFO_SIZE		:= 0x10000
SL_APP_LOD_BASE				:= 0xFFFFFFFF88330000
SL_APP_SIZE         		:= 0x40000
SL_APP1_FLASH_BASE			:= 0xFFFFFFFF88330000
SL_APP2_FLASH_BASE			:= 0xFFFFFFFF88370000
else
SL_FUNCTAB_BASE     		:= 0xFFFFFFFF82500000
SL_FUNCTAB_SIZE     		:= 0x800
SL_APP_BASE         		:= 0xFFFFFFFF82500800
SL_APP_LOADINFO_BASE		:= 0xFFFFFFFF88260000
SL_APP_LOADINFO_SIZE		:= 0x10000
SL_APP_LOD_BASE				:= 0xFFFFFFFF88270000
SL_APP_SIZE         		:= 0x50000
SL_APP1_FLASH_BASE			:= 0xFFFFFFFF88270000
SL_APP2_FLASH_BASE			:= 0xFFFFFFFF882C0000
endif

#specail software version for WOASIS(no bluetooth) S70 + API mode + APART
ifeq "${MODE}" "api"
ifeq "${SL_PLATFORM}" "S70"
SL_APP_LOADINFO_BASE		:= 0xFFFFFFFF882A0000
SL_APP_LOD_BASE				:= 0xFFFFFFFF882B0000
SL_APP1_FLASH_BASE			:= 0xFFFFFFFF882B0000
SL_APP2_FLASH_BASE			:= 0xFFFFFFFF88300000
endif
endif

#specail software version for S70 + BLUETOOTH + API mode + APART
ifeq "${MODE}" "api"
ifeq "${SL_PLATFORM}" "S70"
ifeq "${BLUETOOTH}" "1"
SL_APP_LOADINFO_BASE		:= 0xFFFFFFFF882A0000
SL_APP_LOD_BASE				:= 0xFFFFFFFF882B0000
SL_APP1_FLASH_BASE			:= 0xFFFFFFFF882B0000
SL_APP2_FLASH_BASE			:= 0xFFFFFFFF88300000
endif
endif
endif

endif

